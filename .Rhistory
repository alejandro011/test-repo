testMatrix <- matrix(AirPassengers, byrow = FALSE, ncol = 12)
rownames(testMatrix) <- c(1949:1960)
colnames(testMatrix) <- c(month.abb)
augMean = mean(testMatrix[,"Aug"])
marMean = mean(testMatrix[,"Mar"])
mean1955 = mean(testMatrix["1955",])
mean1960 = mean(testMatrix["1960",])
incomePerYear <- rowSums(testMatrix)
MostProfitableYear = names(incomePerYear[which(incomePerYear == max(incomePerYear))])
LessProfitableYear = names(incomePerYear[which(incomePerYear == min(incomePerYear))])
meanPerYear = incomePerYear / 12
maxMeanPerYear = names(meanPerYear[which(meanPerYear == max(meanPerYear))])
View(testMatrix)
dicMean = mean(testMatrix[,"Dic"])
decMean = mean(testMatrix[,"Dec"])
testMatrix[,"Dec"]
